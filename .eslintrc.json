{
    "env": {
        "node": true,
        "browser": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
      "semi": 1
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
}
